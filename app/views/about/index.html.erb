<!-- START #fh5co-hero -->
<aside id="fh5co-hero">
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
      <% case %>
        <% when params[:locale] == "zh_TW" || params[:locale] == "zh_CN" %>
          <img class="fh5co-hero-wrap" src="/assets/CH_INFORMED Logo.png" alt="informed logo">
        <% when params[:locale] == "vi" %>
          <img class="fh5co-hero-wrap" src="/assets/VIET_INFORMED Logo.png" alt="informed logo">
        <% when params[:locale] == "hmn" %>
          <img class="fh5co-hero-wrap" src="/assets/HMONG_INFORMED Logo.png" alt="informed logo">
        <% when params[:locale] == "en" %>
          <img class="fh5co-hero-wrap" src="/assets/ENG_INFORMED Logo.png" alt="informed logo">
        <% end %>
      </div>
    </div>
  </div>
</aside>

